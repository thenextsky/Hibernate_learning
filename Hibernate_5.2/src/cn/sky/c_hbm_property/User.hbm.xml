<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping auto-import="false">

	<class name="cn.sky.c_hbm_property.User" table="t_user2">
		<id name="id" column="t_id" type="int">
			<generator class="native"></generator>
		</id>
		<property name="name" column="t_name" type="string" not-null="true" length="20"></property>
		<property name="age" column="t_age" type="int" not-null="true"></property>
		<property name="birthday" column="t_birthday" type="date"></property>
		<property name="description" column="`desc`" type="string" length="1000"></property>
		<property name="photo" column="t_photo" type="binary"></property>
		
	</class>

</hibernate-mapping>

